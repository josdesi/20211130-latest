  "adonis-version": "4.1.0",
  "description": "Adonisjs boilerplate for API server with pre-configured JWT",
  "main": "index.js",
  "scripts": {
    "dev": "adonis serve --dev --debug",
    "start": "node server.js",
    "test": "node ace test",
    "test:unit": "node ace test --glob /**/unit_*",
    "test:int": "node ace test --glob /**/int_*",
    "testci": "ENV_SILENT=true node ace test --timeout 45000",
    "debugtest": "ENV_SILENT=true node ace test --timeout 100000000 --files=bulk-email-opt-out-controller.spec.js"
  },
  "keywords": [
    "adonisjs",
    "adonis-app"
  ],
  "author": "",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
